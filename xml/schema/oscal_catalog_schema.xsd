                              <xs:simpleType>
                                 <xs:annotation>
                                    <xs:appinfo>
                                       <m:formal-name>OSCAL version</m:formal-name>
                                       <m:description>The OSCAL model version the document was authored against.</m:description>
                                    </xs:appinfo>
                                    <xs:documentation>
                                       <b>OSCAL version</b>: The OSCAL model version the document was authored against.</xs:documentation>
                                 </xs:annotation>
                                 <xs:restriction base="xs:string"/>
                              </xs:simpleType>
                           </xs:element>
                           <xs:element name="prop"
                                       type="oscal-catalog:property-FIELD"
                                       minOccurs="0"
                                       maxOccurs="unbounded"/>
                           <xs:element name="annotation"
                                       type="oscal-catalog:annotation-ASSEMBLY"
                                       minOccurs="0"
                                       maxOccurs="unbounded"/>
                           <xs:element name="link"
                                       type="oscal-catalog:link-ASSEMBLY"
                                       minOccurs="0"
                                       maxOccurs="unbounded"/>
                           <xs:element name="remarks" minOccurs="0" maxOccurs="1">
                              <xs:annotation>
                                 <xs:appinfo>
                                    <m:formal-name>Remarks</m:formal-name>
                                    <m:description>Additional commentary on the containing object.</m:description>
                                 </xs:appinfo>
                                 <xs:documentation>
                                    <b>Remarks</b>: Additional commentary on the containing object.</xs:documentation>
                              </xs:annotation>
                              <xs:complexType>
                                 <xs:group ref="oscal-catalog:PROSE" maxOccurs="unbounded" minOccurs="0"/>
                              </xs:complexType>
                           </xs:element>
                        </xs:sequence>
                     </xs:complexType>
                  </xs:element>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
         <xs:element name="document-id" minOccurs="0" maxOccurs="unbounded">
            <xs:complexType>
               <xs:annotation>
                  <xs:appinfo>
                     <m:formal-name>Document Identifier</m:formal-name>
                     <m:description>A document identifier qualified by an identifier <m:code>type</m:code>.</m:description>
                  </xs:appinfo>
                  <xs:documentation>
                     <b>Document Identifier</b>: A document identifier qualified by an identifier type.</xs:documentation>
               </xs:annotation>
               <xs:simpleContent>
                  <xs:extension base="xs:string">
                     <xs:attribute name="scheme" use="required" type="oscal-catalog:uri">
                        <xs:annotation>
                           <xs:appinfo>
                              <m:formal-name>Document Identification Scheme</m:formal-name>
                              <m:description>Qualifies the kind of document identifier.</m:description>
                           </xs:appinfo>
                           <xs:documentation>
                              <b>Document Identification Scheme</b>: Qualifies the kind of document identifier.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                  </xs:extension>
               </xs:simpleContent>
            </xs:complexType>
         </xs:element>
         <xs:element name="prop"
                     type="oscal-catalog:property-FIELD"
                     minOccurs="0"
                     maxOccurs="unbounded"/>
         <xs:element name="annotation"
                     type="oscal-catalog:annotation-ASSEMBLY"
                     minOccurs="0"
                     maxOccurs="unbounded"/>
         <xs:element name="link"
                     type="oscal-catalog:link-ASSEMBLY"
                     minOccurs="0"
                     maxOccurs="unbounded"/>
         <xs:element name="role"
                     type="oscal-catalog:role-ASSEMBLY"
                     minOccurs="0"
                     maxOccurs="unbounded"/>
