         <xs:element maxOccurs="unbounded" name="tr" type="oscal-complete:tableRowType"/>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="tableRowType"><!-- QUESTION: Should we allow TH and TD to be mixed? -->
      <xs:choice minOccurs="0" maxOccurs="unbounded">
         <xs:element name="td" type="oscal-complete:tableCellType" maxOccurs="unbounded"/>
         <xs:element name="th" type="oscal-complete:tableCellType" maxOccurs="unbounded"/>
      </xs:choice>
   </xs:complexType>
   <xs:complexType name="tableCellType" mixed="true">
