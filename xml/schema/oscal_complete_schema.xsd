   <xs:group name="blockElementGroup">
      <xs:sequence>
         <xs:choice minOccurs="0" maxOccurs="unbounded">
            <xs:element name="h1" type="oscal-complete:blockElementType"/>
            <xs:element name="h2" type="oscal-complete:blockElementType"/>
            <xs:element name="h3" type="oscal-complete:blockElementType"/>
            <xs:element name="h4" type="oscal-complete:blockElementType"/>
            <xs:element name="h5" type="oscal-complete:blockElementType"/>
            <xs:element name="h6" type="oscal-complete:blockElementType"/>
            <xs:element name="p" type="oscal-complete:blockElementType"/>
            <xs:element name="ul" type="oscal-complete:listType"/>
            <xs:element name="ol" type="oscal-complete:listType"/>
            <xs:element name="pre" type="oscal-complete:preformattedType"/>
            <xs:element name="table" type="oscal-complete:tableType"/>
            <!-- TODO: need to add support for blockquote, which can contain block elements. usnistgov/metaschema#70 -->
         </xs:choice>
      </xs:sequence>
   </xs:group>
   <!--
    headers: phrase inline markup, a, insert (phrase)
    p: phrase inline markup, a, insert, img (phrase+img)
    li: phrase inline markup, a, insert, img (phrase+img)
    pre: phrase inline markup, a, insert (phrase)
    td th: phrase inline markup, a, insert, img (phrase+img)
  -->
   <xs:complexType name="blockElementType" mixed="true">
      <xs:group ref="oscal-complete:phraseInlineMarkupGroup"/>
   </xs:complexType>
   <xs:complexType name="headingType" mixed="true">
      <xs:complexContent>
         <xs:extension base="oscal-complete:blockElementType">
            <xs:annotation>
