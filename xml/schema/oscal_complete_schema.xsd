   <xs:complexType name="inlineMarkupType" mixed="true">
      <xs:group ref="oscal-complete:phraseInlineMarkupGroup"/>
   </xs:complexType>
   <xs:complexType name="imageType">
      <xs:attribute name="alt" type="xs:string"/>
