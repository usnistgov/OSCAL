<?xml version="1.0" encoding="UTF-8"?><x:description xmlns="http://csrc.nist.gov/ns/oscal/1.0"
    xmlns:x="http://www.jenitennison.com/xslt/xspec" xmlns:opr="http://csrc.nist.gov/ns/oscal/profile-resolution" stylesheet="../../oscal-profile-resolve-merge.xsl">
    <x:scenario label="Base">
        <x:context>
            <catalog/>
        </x:context>

        <x:expect label="profile becomes catalog">
            <catalog/>
        </x:expect>
    </x:scenario>

    <x:scenario label="Merge with back matter">
        <x:context>
            <catalog>
                <selection uuid="xyz-tiny_catalog">
                    <group id="x">
                        <title>Group X of XYZ</title>
                        <control id="x1" opr:id="xyz-tiny_catalog#x1"><title>Control X1</title></control>
                    </group>
                    <group id="z">
                        <title>Group Z of XYZ</title>
                        <control id="z1" opr:id="xyz-tiny_catalog#z1"><title>Control Z1</title></control>
                        <control id="z3" opr:id="xyz-tiny_catalog#z3"><title>Control Z3</title>
                            <control id="z3.a" opr:id="xyz-tiny_catalog#z3.a"><title>Control Z3-A</title>
                                <control id="z3.a-1" opr:id="xyz-tiny_catalog#z3.a-1"><title>Control Z3-A-1</title></control>
                            </control>
                        </control>
                    </group>
                    <back-matter>
                        <resource id="ref001">
                            <title>Ref AAA</title>
                            <citation>
                                <text>Aaaa aaaa aaaa</text>
                            </citation>
                            <rlink href="aaa.html"/>
                        </resource>
                        <resource id="ref002">
                            <title>Ref BBB</title>
                            <citation>
                                <text>Bbb bbbbb Bbb b.</text>
                            </citation>
                            <rlink href="bbb.html"/>
                        </resource>
                    </back-matter>
                </selection>

            </catalog>
        </x:context>
        <x:expect label="Losing structure but back matter comes through">
            <catalog>
                <control id="x1"><title>Control X1</title></control>
                <control id="z1"><title>Control Z1</title></control>
                <control id="z3"><title>Control Z3</title>
                    <control id="z3.a"><title>Control Z3-A</title>
                        <control id="z3.a-1">
                            <title>Control Z3-A-1</title></control>
                    </control>
                </control>
                <back-matter>
                    <resource id="ref001">
                        <title>Ref AAA</title>
                        <citation>
                            <text>Aaaa aaaa aaaa</text>
                        </citation>
                        <rlink href="aaa.html"/>
                    </resource>
                    <resource id="ref002">
                        <title>Ref BBB</title>
                        <citation>
                            <text>Bbb bbbbb Bbb b.</text>
                        </citation>
                        <rlink href="bbb.html"/>
                    </resource>
                </back-matter>
            </catalog>
        </x:expect>
    </x:scenario>

    <x:scenario label="Merge as-is with back matter">
        <x:context>
            <catalog>
                <selection uuid="xyz-tiny_catalog">
                    <group id="x">
                        <title>Group X of XYZ</title>
                        <control id="x1" opr:id="xyz-tiny_catalog#x1"><title>Control X1</title></control>
                    </group>
                    <group id="z">
                        <title>Group Z of XYZ</title>
                        <control id="z1" opr:id="xyz-tiny_catalog#z1"><title>Control Z1</title></control>
                        <control id="z3" opr:id="xyz-tiny_catalog#z3"><title>Control Z3</title>
                            <control id="z3.a" opr:id="xyz-tiny_catalog#z3.a"><title>Control Z3-A</title>
                                <control id="z3.a-1" opr:id="xyz-tiny_catalog#z3.a-1"><title>Control Z3-A-1</title></control>
                            </control>
                        </control>
                    </group>
                    <back-matter>
                        <resource id="ref001">
                            <title>Ref AAA</title>
                            <citation>
                                <text>Aaaa aaaa aaaa</text>
                            </citation>
                            <rlink href="aaa.html"/>
                        </resource>
                        <resource id="ref002">
                            <title>Ref BBB</title>
                            <citation>
                                <text>Bbb bbbbb Bbb b.</text>
                            </citation>
                            <rlink href="bbb.html"/>
                        </resource>
                    </back-matter>
                </selection>
                <merge>
                    <as-is>true</as-is>
                </merge>
            </catalog>
        </x:context>
        <x:expect label="Back matter comes through">
            <catalog>
                <group id="x">
                    <title>Group X of XYZ</title>
                    <control id="x1"><title>Control X1</title></control>
                </group>
                <group id="z">
                    <title>Group Z of XYZ</title>
                    <control id="z1"><title>Control Z1</title></control>
                    <control id="z3"><title>Control Z3</title>
                        <control id="z3.a"><title>Control Z3-A</title>
                            <control id="z3.a-1">
                                <title>Control Z3-A-1</title></control>
                        </control>
                    </control>
                </group>
                <back-matter>
                    <resource id="ref001">
                        <title>Ref AAA</title>
                        <citation>
                            <text>Aaaa aaaa aaaa</text>
                        </citation>
                        <rlink href="aaa.html"/>
                    </resource>
                    <resource id="ref002">
                        <title>Ref BBB</title>
                        <citation>
                            <text>Bbb bbbbb Bbb b.</text>
                        </citation>
                        <rlink href="bbb.html"/>
                    </resource>
                </back-matter>
            </catalog>
        </x:expect>
    </x:scenario>
</x:description>
